{
  "name": "@gdulik/react-native-version-up",
  "version": "1.1.2",
  "description": "Increase version and build number in your react native app for package.json and both platforms (ios && android) with one command",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "rebuild": "yarn clean && yarn build",
    "clean": "npx rimraf lib",
    "test": "npx jest --config=jest.config.js --passWithNoTests",
    "lint": "npx eslint -c .eslintrc.js ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gdulik/react-native-version-up.git"
  },
  "keywords": [
    "react native version",
    "react native version up"
  ],
  "author": "Tomas Gdula",
  "license": "ISC",
  "dependencies": {
    "yargs": "17.6.2"
  },
  "devDependencies": {
    "@foxmedia/eslint-config": "git+ssh://git@gitlab.com:foxmedia/eslint-config.git",
    "@foxmedia/prettier-config": "git+ssh://git@gitlab.com:foxmedia/prettier-config.git",
    "@types/jest": "29.2.3",
    "@types/yargs": "17.0.14",
    "eslint": "8.28.0",
    "jest": "29.3.1",
    "prettier": "2.8.0",
    "rimraf": "3.0.2",
    "ts-jest": "29.0.3",
    "typescript": "4.9.3",
    "yarn": "1.22.0"
  }
}
